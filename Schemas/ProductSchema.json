{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Product",
    "description": "A product from Acme's catalog",
    "type": "object",
    "properties": {
        "z": {
            "description": "default payout start date",
            "type": "string",
            "format": "date"
        },
        "age": {
            "description": "age of insured person",
            "type": "number",
            "minimum": 0,
            "maximum": 120
        },
        "premiumAmount": {
            "description": "premium amount",
            "type": "number",
            "minimum": 100,
            "maximum": 100000
        },
        "taxCategory": {
            "enum": [
                "P",
                "T",
                "K"
            ]
        },
        "survivorProtection": {
            "type": "boolean"
        }
    },
    "required": [
        "z",
        "age",
        "taxCategory"
    ]
}