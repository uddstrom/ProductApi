{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Product",
    "description": "A product config catalog",
    "type": "object",
    "properties": {
        "survivorProtection": {
            "type": "boolean"
        },
        "z": {
            "description": "default payout start date",
            "type": "string",
            "format": "date"
        },
        "birthDate": {
            "description": "birthDate of insured person",
            "type": "string",
            "format": "date"
        },
        "taxCategory": {
            "enum": [
                "P",
                "T",
                "K"
            ]
        },
        "benefitProductId": {
            "description": "identifierare för förmånsprodukten",
            "type": "string"
        },
        "maturity": {
            "description": "försäkringens ålder",
            "type": "number",
            "minimum": 0
        }
    },
    "required": [
        "z",
        "birthDate",
        "taxCategory",
        "benefitProductId",
        "maturity"
    ]
}